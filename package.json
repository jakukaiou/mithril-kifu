{
  "name": "typescript_sass_webpack_boilerplate",
  "version": "2.0.0",
  "description": "A simple webpack boilerplate for front end projects using Typescript/Sass",
  "keywords": [
    "typescript",
    "sass",
    "webpack"
  ],
  "main": "index.js",
  "scripts": {
    "test": "karma start karma.conf.js",
    "tslint": "tslint",
    "stylelint": "stylelint",
    "server": "webpack-dev-server --hot --inline --content-base --client-log-level warning",
    "start": "npm run server",
    "build": "webpack --progress --colors --display-modules",
    "typings": "typings"
  },
  "author": "Ryan Ruan",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "chai": "^4.0.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.1.2",
    "node-sass": "^4.5.3",
    "phantomjs": "^2.1.7",
    "postcss-loader": "^2.0.5",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "ts-loader": "^2.1.0",
    "tslint": "^5.3.2",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.3.4",
    "typings": "^2.1.1",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "normalize-scss": "^7.0.0"
  }
}
