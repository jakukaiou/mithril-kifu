{
  "name": "typescript_sass_webpack_boilerplate",
  "version": "2.0.2",
  "description": "A simple webpack boilerplate for front end projects using Typescript/Sass",
  "keywords": [
    "typescript",
    "sass",
    "webpack"
  ],
  "main": "index.js",
  "scripts": {
    "test": "karma start karma.conf.js",
    "tslint": "tslint",
    "stylelint": "stylelint",
    "server": "webpack-dev-server --hot --inline --content-base --client-log-level warning",
    "start": "npm run server",
    "build": "webpack --progress --colors --display-modules",
    "typings": "typings"
  },
  "author": "Ryan Ruan",
  "license": "MIT",
  "devDependencies": {
    "@types/classnames": "^2.2.0",
    "@types/firebase": "^2.4.32",
    "@types/lodash": "^4.14.71",
    "@types/mithril": "^1.1.7",
    "@types/moment": "^2.13.0",
    "autoprefixer": "^7.1.1",
    "awesome-typescript-loader": "^3.2.2",
    "chai": "^4.0.0",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.1.2",
    "node-sass": "^4.5.3",
    "phantomjs-prebuilt": "^2.1.14",
    "postcss-loader": "^2.0.5",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.5",
    "style-loader": "^0.18.1",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-webpack-plugin": "^0.7.0",
    "tslint": "^5.4.2",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-loader": "^3.5.3",
    "typescript": "^2.3.4",
    "typings": "^2.1.1",
    "url-loader": "^0.5.9",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5",
    "webpack-obfuscator": "^0.11.0"
  },
  "dependencies": {
    "bulma": "^0.6.0",
    "bulma-switch": "0.0.4",
    "bulma-tooltip": "0.0.4",
    "classnames": "^2.2.5",
